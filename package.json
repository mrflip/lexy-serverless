{
  "name": "tooksome-samwise",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:mrflip/samwise.git"
  },
  "scripts": {
    "test": "jest",
    "dynamodb-generate-seed-data": "cd seed-data && node create_seed_data.js",
    "dynamodb-insert-seed-data-prod": "cd seed-data && node insert_seed_data_prod.js",
    "start": "yarn dynamodb-generate-seed-data && serverless dynamodb install && yarn launch",
    "launch": "env-cmd -f ./.secrets.env.local serverless offline start",
    "deploy-prod": "yarn dynamodb-generate-seed-data && env-cmd -f .secrets.env.prod serverless --stage=production deploy && yarn dynamodb-insert-seed-data-prod"
  },
  "dependencies": {
    "apollo-server-lambda": "^2.9.16",
    "aws-xray-sdk": "^2.5.0",
    "babel-polyfill": "^6.26.0",
    "graphql": "^14.6.0",
    "serverless-dynamodb-client": "^0.0.2"
  },
  "devDependencies": {
    "apollo-server-module-graphiql": "^1.4.0",
    "aws-sdk": "^2.610.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "env-cmd": "^10.0.1",
    "faker": "^4.1.0",
    "imports-loader": "^0.8.0",
    "jest": "^25.1.0",
    "jsonfile": "^5.0.0",
    "serverless-dynamodb-local": "^0.2.39",
    "serverless-offline": "^5.12.1",
    "serverless-webpack": "^5.3.1",
    "webpack": "^4.41.5",
    "webpack-node-externals": "^1.7.2"
  }
}
